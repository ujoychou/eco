<?xml version="1.0" encoding="utf-8"?>
<root>
  <!--#.配置文件-->
  <config>
    <file_path>eco.xml</file_path>
  </config>

  <!--#.日志选项-->
  <logging>
    <async value="true"/>
    <async_flush value="3000"/>
    <memcache value="5M"/>
    <file_sink enable="true">
      <level value="debug"/>
      <file_path>./log/</file_path>
      <roll_size>100M</roll_size>
      <full_path>20170104-160222.ujoychou-PC.00051124.log</full_path>
    </file_sink>
    <console_sink enable="false">
      <level value="info"/>
    </console_sink>
  </logging>

  <!--#.生命机制-->
  <eco enable="true">
    <being enable="true">
      <unit_live_tick>5</unit_live_tick>
    </being>
  </eco>

  <!--#.插件管理-->
  <erx enable="false">
  </erx>
</root>